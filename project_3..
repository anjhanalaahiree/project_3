27|const Onboarding = ({ onComplete }) => {
28|  const [formData, setFormData] = useState({
29|    name: "",
30|    email: "",
31|    user_type: "household"
32|  });
33|  const [loading, setLoading] = useState(false);
34|
35|  const handleSubmit = async (e) => {
36|    e.preventDefault();
37|    setLoading(true);
38|    
